{
  "input": {
    "model": "{{ model }}",
    "version": "{{ version }}",
    "raw": "{{ review_raw }}",
    "truth": {{ truth }},
    "preprocessed": "{{ review_preprocessed }}",
    "encoded": {{ review_encoded }}
  },
  {%- if status == "SUCCESS" %}
    "prediction": {
      "star_rating": {{ rating }},
      "raw": {{ prediction_raw }}
    },
  {%- else %}
      "error_message": "{{ error_message }}",
  {%- endif %}
  "status": "{{ status }}",
  "timestamp": "{{ timestamp }}"
}